{
    "name": "nicotec/doctrineauto-bundle",
    "type": "symfony-bundle",
    "description": "Generator Entity Doctrine for Mysql - doctrine reverse engineer auto",
    "keywords": ["generator", "doctrine2", "mysql"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Longuet",
            "email": "nicolas.longuet@vivaneo.fr"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.3",
        "twig/twig": "*",
        "doctrine/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-0": {
            "Nicotec\\DoctrineautoBundle\\": ""
        }
    },
    "target-dir": "Nicotec/DoctrineautoBundle",
    "extra": {
        "symfony-assets-install": "relative"
    },
    "minimum-stability": "master"
}
